{
  "name": "@boombox/api",
  "version": "1.0.0",
  "description": "API for Boombox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ian97531/boombox.git"
  },
  "keywords": [
    "podcast",
    "transcription"
  ],
  "author": "Ian White, Andrew Hoyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ian97531/boombox/issues"
  },
  "homepage": "https://github.com/ian97531/boombox#readme",
  "scripts": {
    "format": "prettier --write --config .prettierrc.yaml '**/*.ts'",
    "clean": "rm -f src/*.js && rm -f src/*.js.map",
    "lint": "tslint 'src/**/*.ts'",
    "start": "ts-node src/local.ts",
    "build": "yarn clean && tsc",
    "domain": "serverless create_domain",
    "package": "yarn build && serverless package",
    "deploy": "yarn build && serverless deploy"
  },
  "dependencies": {
    "@boombox/shared": "1.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/validator": "^9.4.1",
    "aws-sdk": "^2.296.0",
    "aws-serverless-express": "^3.2.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "global": "^4.3.2",
    "source-map-support": "^0.5.8",
    "validator": "^10.6.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.11",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/cors": "^2.8.4",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.13.0",
    "prettier": "^1.14.2",
    "serverless-domain-manager": "^2.6.0",
    "serverless-offline": "^3.25.7",
    "serverless-plugin-typescript": "^1.1.5",
    "ts-node": "^5.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^2.7.2"
  }
}
