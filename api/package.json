{
  "name": "@boombox/api",
  "version": "1.0.0",
  "description": "API for Boombox",
  "repository": "https://github.com/gg-incorporated/boombox",
  "keywords": [
    "podcast",
    "transcription"
  ],
  "author": "Ian White, Andrew Hoyer",
  "bugs": {
    "url": "https://github.com/gg-incorporated/boombox/issues"
  },
  "private": true,
  "license": "UNLICENSED",
  "homepage": "https://github.com/gg-incorporated/boombox/tree/master/api#readme",
  "scripts": {
    "format": "prettier --write --config .prettierrc.yaml '**/*.ts'",
    "clean-all": "yarn clean-shared && yarn clean",
    "clean-shared": "cd node_modules/@boombox/shared && yarn clean",
    "clean": "rm -rf build/dist",
    "lint": "tslint 'src/**/*.ts'",
    "start": "ts-node src/local.ts",
    "build-all": "yarn build-shared && yarn build",
    "build-shared": "cd node_modules/@boombox/shared && yarn build",
    "build": "echo 'Building @boombox/api with tsc' && tsc --version && && tsc",
    "domain": "serverless create_domain",
    "package": "yarn clean-all && yarn build-all && serverless package",
    "deploy": "yarn clean-all && yarn build-all && serverless deploy"
  },
  "dependencies": {
    "@boombox/shared": "1.0.0",
    "@types/aws-lambda": "^8.10.11",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/cors": "^2.8.4",
    "@types/validator": "^9.4.1",
    "aws-sdk": "^2.336.0",
    "aws-serverless-express": "^3.2.0",
    "axios": "^0.18.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "global": "^4.3.2",
    "source-map-support": "^0.5.8",
    "validator": "^10.6.0"
  },
  "devDependencies": {
    "lint-staged": "^7.2.2",
    "nodemon": "^1.13.0",
    "prettier": "^1.14.2",
    "serverless-domain-manager": "^2.6.0",
    "serverless-offline": "^3.25.7",
    "serverless-plugin-typescript": "^1.1.5",
    "ts-node": "^5.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.7.2"
  }
}
