{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-redux": "^6.0.6",
    "husky": "^0.14.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier-tslint --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "copyToS3": "serverless client deploy --no-config-change --no-cors-change --no-policy-change --no-confirm",
    "domain": "serverless deploy",
    "deploy": "yarn build && yarn domain && yarn copyToS3",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "lint-staged": "^7.2.0",
    "prettier-tslint": "^0.4.0",
    "serverless-finch": "^2.2.0",
    "typescript": "^3.0.1"
  }
}
