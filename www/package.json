{
  "name": "@boombox/www",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/gg-incorporated/boombox/tree/master/www#readme",
  "dependencies": {
    "@boombox/shared": "1.0.0",
    "@types/query-string": "^6.1.0",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.3.0",
    "axios": "^0.18.0",
    "husky": "^0.14.3",
    "normalize.css": "^8.0.0",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts-ts start",
